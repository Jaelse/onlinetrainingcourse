<h1>Search Result</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Citizen ID</th>
      <th>Student ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subscription_filtered.each do |subscription| %>
      <tr>
        <td><%= Subscriber.find(subscription.subscriber_id).fname %></td>
        <td><%= Subscriber.find(subscription.subscriber_id).lname %></td>
        <td><%= Subscriber.find(subscription.subscriber_id).citizenid %></td>
        <td><%= Subscriber.find(subscription.subscriber_id).insorstid %></td>
         <td><%= link_to 'Show', Subscriber.find( subscription.subscriber_id) %></td>
        <td><%= link_to 'Edit', edit_subscriber_path(Subscriber.find( subscription.subscriber_id)) %></td>
        <td><%= link_to 'Destroy', Subscriber.find( subscription.subscriber_id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "back", students_list_path %>